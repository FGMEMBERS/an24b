<PropertyList>

<!-- Fuel control panel -->

  <path>fuelcontrol.ac</path>

 <animation>
  <type>material</type>
  <object-name>background</object-name>
  <condition>
   <equals>
    <property>an24/lights/instrlights</property>
    <value>1.0</value>
   </equals>
  </condition>
  <emission>
   <red>1.0</red>
   <green>0.5</green>
   <blue>0.5</blue>
   <factor-prop>an24/lights/instrlights</factor-prop>
  </emission>
 </animation>

