<?xml version="1.0"?>

<!-- AP horizon button -->

<PropertyList>
 <path>../../shared/switches/spring_load_btn02.ac</path>

<animation>
  <type>pick</type>
  <object-name>press_me</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-toggle</command>
      <property>an24/ap-28l1/buttons/horizon</property>
    </binding>
  </action>
</animation>

<animation>
  <type>translate</type>
  <object-name>press_me</object-name>
  <property>an24/ap-28l1/buttons/horizon</property>
  <factor>0.003</factor>
  <axis>
    <x>0</x>
    <y>0</y>
    <z>-1</z>
  </axis>
</animation>

</PropertyList>

