<?xml version="1.0"?>

<!-- VAR-30, FO's variometer -->

<PropertyList>
 <path>var-30.ac</path>

 <animation>
   <type>material</type>
   <object-name>housing</object-name>
   <emission>
     <red>1.0</red>
     <green>1.0</green>
     <blue>1.0</blue>
     <factor-prop>an24/lights/instrlights</factor-prop>
   </emission>
 </animation>

<!--Needles-->
<animation>
  <type>rotate</type>
  <object-name>long_needle</object-name>
  <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
  <factor>-0.093</factor>
  <min>-180</min>
  <max>180</max>
  <axis>
    <x>1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>
</PropertyList>
