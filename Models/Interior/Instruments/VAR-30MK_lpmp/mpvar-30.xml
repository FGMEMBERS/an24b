<?xml version="1.0"?>

<!-- VAR-30, FO's variometer -->

<PropertyList>
 <path>var-30.ac</path>

 <animation>
   <type>material</type>
   <object-name>housing</object-name>
   <object-name>needle</object-name>
   <emission>
     <red>1.0</red>
     <green>1.0</green>
     <blue>1.0</blue>
     <factor-prop>an24/lights/instrlights</factor-prop>
   </emission>
 </animation>

<!--Needles-->
<animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
    <interpolation>
  	<entry><ind>	5708.66</ind>	<dep>	177.0</dep></entry>
	<entry><ind>	3937.0</ind>	<dep>	142.0</dep></entry>
	<entry><ind>	1968.5</ind>	<dep>	83.0</dep></entry>
	<entry><ind>	984.25</ind>	<dep>	44.0</dep></entry>
	<entry><ind>	0.0</ind>	<dep>	0.0</dep></entry>
        <entry><ind>	-984.25</ind>	<dep>	-44.0</dep></entry>
        <entry><ind>	-1968.5</ind>	<dep>	-83.0</dep></entry>
        <entry><ind>	-3937.0</ind>	<dep>	-142.0</dep></entry>
        <entry><ind>	-5708.66</ind>	<dep>	-177.0</dep></entry>
    </interpolation>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
</animation>

