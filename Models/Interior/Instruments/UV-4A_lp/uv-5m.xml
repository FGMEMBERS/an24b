<?xml version="1.0"?>

<!-- Pilot's radio altimeter -->

<PropertyList>
 <path>uv-5m.ac</path>

 <animation>
   <type>material</type>
   <object-name>housing</object-name>
   <object-name>needle</object-name>
   <object-name>cover</object-name>
   <emission>
     <red>1.0</red>
     <green>0.3</green>
     <blue>0.3</blue>
     <factor-prop>an24/lights/instrlights</factor-prop>
   </emission>
 </animation>

 <animation>
   <type>material</type>
   <object-name>flag</object-name>
   <emission>
     <red>0.5</red>
     <green>0.2</green>
     <blue>0.2</blue>
     <factor-prop>an24/lights/instrlights</factor-prop>
   </emission>
 </animation>


</PropertyList>
