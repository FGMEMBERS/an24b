 <animation>
   <type>knob</type>
   <object-name>b2_lockD</object-name>
     <condition>
       <property>/devices/status/keyboard/shift</property>
     </condition>
   <property>an24/R-836/b2_lockD</property>
   <factor>30</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>-0.0033</y-m>
   <z-m>0.0898</z-m>
  </center>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
    <action>
     <binding>
      <command>property-adjust</command>
      <property>an24/R-836/b2_lockD</property>
      <factor>1</factor>
      <min>0</min>
      <max>1</max>
      <wrap>false</wrap>
     </binding>
    </action>
 </animation>

