 <animation>
  <object-name>b2_hood</object-name>
  <object-name>b2_fixatorL</object-name>
  <object-name>b2_fixatorR</object-name>
  <type>select</type>
  <condition>
   <not>
    <property>/sim/signals/fdm-initialized</property>
   </not>
  </condition>
 </animation>

