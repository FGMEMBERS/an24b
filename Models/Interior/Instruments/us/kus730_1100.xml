<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>kus730_1100.ac</path>
  
  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>long_needle</object-name>
    <object-name>short_needle</object-name>
    <object-name>cache</object-name>
    
    <object-name>glass</object-name>
  </animation>
  
  <animation>
    <type>noshadow</type>
    <object-name>glass</object-name>
  </animation>
  
  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>glass</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <condition>
      <property>sim/rendering/rembrandt/enabled</property>
    </condition>
    <object-name>glass</object-name>
  </effect>
  
  <!-- Internal instruments lighting -->
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>long_needle</object-name>
    <object-name>short_needle</object-name>
    <object-name>cache</object-name>
   <emission>
     <red>1.0</red>
     <green>1.0</green>
     <blue>1.0</blue>
     <factor-prop>an24/lights/instrlights</factor-prop>
   </emission>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>short_needle</object-name>
    <property>instrumentation/airspeed-indicator/true-speed-kt</property>
    <interpolation>
      <entry><ind>	199.0</ind>	<dep>	180.0</dep></entry>
      <entry><ind>	215.98</ind>	<dep>	196.0</dep></entry>
      <entry><ind>	269.98</ind>	<dep>	244.0</dep></entry>
      <entry><ind>	323.97</ind>	<dep>	292.0</dep></entry>
      <entry><ind>	377.97</ind>	<dep>	338.0</dep></entry>
      <entry><ind>	593.95</ind>	<dep>	527.0</dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
  <type>rotate</type>
  <object-name>long_needle</object-name>
  <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
  <interpolation>
    <entry><ind>	27.0</ind>	<dep>	5.0</dep></entry>
    <entry><ind>	54.0</ind>	<dep>	23.0</dep></entry>
    <entry><ind>	108.0</ind>	<dep>	75.0</dep></entry>
    <entry><ind>	215.98</ind>	<dep>	181.0</dep></entry>
    <entry><ind>	323.97</ind>	<dep>	274.0</dep></entry>
    <entry><ind>	404.96</ind>	<dep>	340.0</dep></entry>
  </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>cache</object-name>
    <condition>
      <less-than>
        <property>fdm/jsbsim/velocities/vt-fps</property>
        <value>399.9999</value>
      </less-than>
    </condition>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
