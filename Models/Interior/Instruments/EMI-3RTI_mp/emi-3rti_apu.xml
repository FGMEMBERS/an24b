<?xml version="1.0"?>

<!-- EMI-3RTI, APU oil pressure and temperature
and fuel pressure indicator -->

<PropertyList>
 <path>emi-3rti_6cm.ac</path>

 <animation>
   <type>material</type>
   <object-name>pressure_1</object-name>
   <object-name>pressure_2</object-name>
   <object-name>temperature</object-name>
   <object-name>housing</object-name>
   <emission>
     <red>1.0</red>
     <green>0.3</green>
     <blue>0.3</blue>
     <factor-prop>an24/lights/instrlights</factor-prop>
   </emission>
 </animation>

</PropertyList>
