<?xml version="1.0"?>

<!-- RTMS-0.85-B1, engine 1 fuel flow-->

<PropertyList>
 <path>rtms.ac</path>

 <animation>
   <type>material</type>
   <object-name>housing</object-name>
   <object-name>needle</object-name>
   <object-name>cover</object-name>
   <object-name>dig100</object-name>
   <object-name>dig10</object-name>
   <object-name>dig1</object-name>
   <emission>
     <red>1.0</red>
     <green>0.3</green>
     <blue>0.3</blue>
     <factor-prop>an24/lights/instrlights</factor-prop>
   </emission>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>needle</object-name>
  <property>fdm/jsbsim/propulsion/engine[0]/fuel-flow-rate-pps</property>
  <interpolation>
    <entry><ind>	0.0</ind>	<dep> -148.0</dep></entry>
    <entry><ind>        0.12236</ind>	<dep> -113.0</dep></entry>
    <entry><ind>        0.48947</ind>	<dep>  114.0</dep></entry>
  </interpolation>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>

</PropertyList>
