  <animation>
  <type>rotate</type>
  <object-name>rect2</object-name>
  <property>IL-76/systems/tks/kush3/indicated-heading-deg</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>    0</dep></entry> 
   <entry><ind> 90</ind><dep>  90</dep></entry>
   <entry><ind> 180</ind><dep>  180</dep></entry>
   <entry><ind> 270</ind><dep>  270</dep></entry>
   <entry><ind> 360</ind><dep>  360</dep></entry>
 
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  
</animation>

  <animation>
  <type>rotate</type>
  <object-name>rect1</object-name>
  <property>IL-76/systems/tks/kush3/indicated_1</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>    0</dep></entry> 
   <entry><ind> 90</ind><dep>  90</dep></entry>
   <entry><ind> 180</ind><dep>  180</dep></entry>
   <entry><ind> 270</ind><dep>  270</dep></entry>
   <entry><ind> 360</ind><dep>  360</dep></entry>
 
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  
</animation>


 <animation>
  <type>textranslate</type>
  <object-name>L1</object-name>
  <property>IL-76/systems/tks/kush3/mode_M</property>
  <factor>1.0</factor>
 
  <interpolation>
    <entry>
      <ind>0.0</ind>
      <dep>0.0</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>0.50</dep>
    </entry>
   
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<animation>
	<type>material</type>
	<object-name>L1</object-name>
	<emission>
		<red>0.8</red>
		<green>0.7</green>
		<blue>0.7</blue>
		<factor-prop>IL-76/systems/tks/kush3/mode_M</factor-prop>
	</emission>
</animation>

<animation>
  <type>textranslate</type>
  <object-name>L2</object-name>
  <property>IL-76/systems/tks/kush3/mode_G</property>
  <factor>1.0</factor>
 
  <interpolation>
    <entry>
      <ind>0.0</ind>
      <dep>0.0</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>0.50</dep>
    </entry>
   
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<animation>
	<type>material</type>
	<object-name>L2</object-name>
	<emission>
		<red>0.8</red>
		<green>0.7</green>
		<blue>0.7</blue>
		<factor-prop>IL-76/systems/tks/kush3/mode_G</factor-prop>
	</emission>
</animation>


 
 <!--Lights-->
                                   
  <animation>
   <type>material</type>
   <object-name>rect1</object-name>
   <emission>
		<red-prop>/IL-76/instrumentation/lights/panelN/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/panelN/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/panelN/int-blue</blue-prop>
	</emission>
</animation>

  <animation>
   <type>material</type>
   <object-name>rect2</object-name>
   <emission>
		<red-prop>/IL-76/instrumentation/lights/panelN/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/panelN/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/panelN/int-blue</blue-prop>
	</emission>
</animation>


<animation>
   <type>material</type>
   <object-name>b1</object-name>
   <emission>
		<red-prop>/IL-76/instrumentation/lights/panelN/int-red</red-prop>
    <green-prop>/IL-76/instrumentation/lights/panelN/int-green</green-prop>
    <blue-prop>/IL-76/instrumentation/lights/panelN/int-blue</blue-prop>
	</emission>
</animation>

 


 </PropertyList>

