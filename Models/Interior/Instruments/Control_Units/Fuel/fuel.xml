<?xml version="1.0"?>

<!-- 3 lamps at top right of middle panel
     - low on fuel
     - failure of L/R fuel filter -->

<PropertyList>

<model>
 <path>mp_wl_01_low_fuel.xml</path>
  <offsets>
    <x-m>2.09232</x-m>
    <y-m>0.1985</y-m>
    <z-m>1.079</z-m>
  </offsets>
</model>

<model>
 <path>mp_wl_02_fuel_filter_clogged_left.xml</path>
  <offsets>
    <x-m>2.09232</x-m>
    <y-m>0.1869</y-m>
    <z-m>1.044</z-m>
  </offsets>
</model>

<model>
 <path>mp_wl_03_fuel_filter_clogged_right.xml</path>
  <offsets>
    <x-m>2.09232</x-m>
    <y-m>0.208</y-m>
    <z-m>1.044</z-m>
  </offsets>
</model>

<!-- TG-16 cutoff-valve and valve state signal lamps on horizontal left console -->

<model>
