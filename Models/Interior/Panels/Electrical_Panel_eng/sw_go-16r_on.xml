<?xml version="1.0"?>

<!-- Right reserve GO-16 generator ON -->

<PropertyList>
 <path>../../shared/switches/png-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Electrical_Panel/sw_go16r", 1.0, 0.1 );
	settimer(func {electrical.go16r.start();}, 1.0);
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>go16ron</tooltip-id>
      <label>Reserve Generator
    GO-16 ON</label>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        interpolate("an24/Electrical_Panel/sw_go16r", 0.0, 0.1 );
	settimer(func {electrical.go16r.stop();}, 1.0);
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>go16roff</tooltip-id>
      <label>Reserve Generator
    GO-16 OFF</label>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>an24/Electrical_Panel/sw_go16r</property>
  <interpolation>
    <entry><ind>1.0</ind>	<dep>20.0</dep></entry>
    <entry><ind>0.0</ind>	<dep>-20.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>
</PropertyList>
